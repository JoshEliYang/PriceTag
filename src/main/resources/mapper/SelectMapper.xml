<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.springmvc.dao.SelectDAO">

	<select id="selectAllShop" resultType="Select">
		SELECT DISTINCT shopId AS id, CASE shopId 
		  WHEN  'KJG001' THEN '杨浦'
          WHEN  'KJG002' THEN '大华' 
          WHEN  'KJG003' THEN '徐汇'
          ELSE '松江' END AS content
          FROM PriceTagInfo;
	</select>
	
	<select id="selectAllOrigin" resultType="String">
		SELECT DISTINCT goodsOrigin FROM PriceTagInfo;
	</select>
</mapper>
